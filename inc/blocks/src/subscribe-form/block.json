{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "glimp/subscribe-form",
	"version": "1.0.0",
	"title": "Subscribe form",
	"category": "theme",
	"description": "The part of the Glimp theme.",
	"attributes": {
		"button": {
			"type": "string",
			"source": "html",
			"selector": ".wp-block-glimp-subscribe-form__button"
		},
		"placeholder": {
			"type": "string",
			"source": "attribute",
			"selector": ".wp-block-glimp-subscribe-form__input",
			"attribute": "placeholder"
		}
	},
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "glimp/subscribe-form",
	"editorScript": "file:./index.js",
    "style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
