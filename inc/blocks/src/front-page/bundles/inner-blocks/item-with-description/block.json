{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "glimp/bundles-item-with-description",
	"version": "1.0.0",
	"title": "Item with description",
	"category": "theme",
	"icon": "align-left",
	"description": "The part of the Bundles block.",
	"attributes": {
		"title": {
			"type": "string",
			"source": "html",
			"selector": ".wp-block-glimp-bundles-item-with-description__title"
		},
		"text": {
			"type": "string",
			"source": "html",
			"selector": ".wp-block-glimp-bundles-item-with-description__text"
		},
		"button": {
			"type": "string",
			"source": "html",
			"selector": ".wp-block-glimp-bundles-item-with-description__button"
		},
		"mediaID": {
			"type": "number"
		},
		"mediaURL": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src"
		}
	},
	"example": {},
	"parent": [
		"glimp/bundles"
	],
	"supports": {
		"html": false,
		"color": {
			"background": true
		}
	},
	"textdomain": "glimp/bundles-item-with-description",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}