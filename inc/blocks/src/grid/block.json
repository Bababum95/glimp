{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "glimp/grid",
	"version": "1.0.0",
	"title": "Grid container",
	"category": "design",
	"icon": "grid-view",
	"description": "The part of the Glimp theme.",
	"attributes": {
		"gap": {
			"type": "object",
			"default": {
				"row": {
					"desktop": "20px",
					"phone": "20px"
				},
				"column": {
					"desktop": "20px",
					"phone": "20px"
				}
			}
		},
		"count": {
			"type": "object",
			"default": {
				"desktop": 3,
				"phone": 2
			}
		}
	},
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "glimp/grid",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
