{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "glimp/marquee",
	"version": "1.0.0",
	"title": "Marquee",
	"category": "theme",
	"icon": "editor-insertmore",
	"description": "The part of the Glimp theme.",
	"attributes": {
		"text": {
			"type": "string",
			"source": "html",
			"selector": "p"
		},
		"canClose": {
			"type": "boolean",
			"default": true
		},
		"submarquee": {
			"type": "boolean",
			"default": false
		},
		"delay": {
			"type": "number",
			"default": 0
		},
		"showAnimation": {
			"type": "boolean",
			"default": false
		}
	},
	"example": {},
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"text": true
		},
		"spacing": {
			"margin": false,
			"padding": true
		}
	},
	"textdomain": "glimp/marquee",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
    "style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}