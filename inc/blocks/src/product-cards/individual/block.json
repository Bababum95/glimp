{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "glimp/individual-product-card",
	"version": "1.0.0",
	"title": "Individual product card",
	"category": "theme",
	"description": "The part of the Glimp theme.",
	"attributes": {
		"id": {
			"type": "number",
			"default": 0
		},
		"variation": {
			"type": "boolean",
			"default": true
		},
		"count": {
			"type": "number",
			"default": 0
		}
	},
	"example": {},
	"supports": {
		"html": false
	},
	"parent": ["glimp/slider"],
	"textdomain": "glimp/individual-product-card",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}