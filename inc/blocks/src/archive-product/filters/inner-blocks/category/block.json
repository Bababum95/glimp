{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "glimp/filters-category",
	"version": "1.0.0",
	"icon": "admin-post",
	"title": "Filters category",
	"category": "theme",
	"description": "The part of the Glimp theme.",
	"attributes": {
		"placeholder": {
			"type": "string",
