{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "glimp/filters-item",
	"version": "1.0.0",
	"icon": "arrow-down",
	"title": "Filters item",
	"category": "theme",
