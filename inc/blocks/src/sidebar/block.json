{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "glimp/sidebar",
	"version": "1.0.0",
	"title": "Sidebar",
	"category": "theme",
	"description": "The part of the Glimp theme.",
	"attributes": {
		"links": {
			"type": "array",
			"source": "query",
			"selector": "li",
			"query": {
				"value": {
					"type": "string",
					"source": "html"
				}
			},
			"default": [
				{
					"value": ""
				}
			]
		}
	},
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "glimp/slider",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
    "style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
