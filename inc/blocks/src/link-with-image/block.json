{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "glimp/link-with-image",
	"version": "1.0.0",
	"title": "Link with image",
	"category": "theme",
	"description": "The part of the Glimp theme.",
	"attributes": {
		"path": {
			"type": "string",
			"default": "",
			"source": "attribute",
			"selector": "a",
			"attribute": "href"
		},
		"fullPage": {
			"type": "boolean",
			"default": false
		},
		"target": {
			"type": "boolean",
			"default": false
		}
	},
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "glimp/link-with-image",
	"editorScript": "file:./index.js",
    "style": "file:./style-index.css"
}
